{
  "name": "xmtp-mini-app",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "concurrently \"next dev -p 3000\" \"yarn dev:backend\"",
    "dev:backend": "cd backend && tsx --watch index.ts",
    "dev:ngrok": "tsx scripts/dev-with-ngrok.ts",
    "lint": "next lint",
    "start": "concurrently \"next start\" \"yarn start:backend\"",
    "start:backend": "cd backend && tsx index.ts"
  },
  "dependencies": {
    "@farcaster/frame-core": "^0.0.35",
    "@farcaster/frame-node": "^0.0.24",
    "@farcaster/frame-sdk": "^0.0.41",
    "@ngrok/ngrok": "^1.5.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@xmtp/agent-sdk": "^1.1.7",
    "clsx": "^2.1.1",
    "eruda": "^3.4.1",
    "jiti": "1.21.7",
    "ky": "^1.8.1",
    "next": "14.2.6",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": ">=20"
  }
}
